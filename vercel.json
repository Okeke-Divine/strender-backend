{
    "builds": [{
        "src": "strender_backend/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "pythhon3.8"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "strender_backend/wsgi.py"
        }
    ]
}